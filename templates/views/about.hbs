     ></script>

       <!-- Link Swiper's CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.css" />
   <title>{{title}}</title>
</head>
<body>
    {{>navbar}}
     <div class="section">
      <div class="Acontainer">
        <div class="title">
          <h1>About Us</h1>
        </div>
        <div class="content">
            <div class="airticle">
                <h3>President</h3>
                <br>
                <h4>Prof.Puneet Bisaria</h>
                <br>
                <span>Noted  author  and  Film  Critic  Bundelkhand   University , Jhansi.</sapn>
            </div>
        </div>
        <div class="img-section">
            <img src="images/b1.jpg" alt="">
        </div>
        <div class="social">
            <a href="#"><i class="fa-brands fa-twitter"></i></a>
            <a href="#"><i class="fa-brands fa-instagram"></i></a>
            <a href="#"><i class="fa-brands fa-youtube"></i></a>
        </div>
      </div>
    </div>

    <section class="visePresident">
        <div class="Acontainer">
            <div class="visePresident1">
                <img src="../images/b1.jpg" alt="">
                <h3>Vise President</h3>
                <span>Shri Dhirendra Pratap Srivastav</span>
                <span>(Advocate-Supreme Court Of India)</span>
            </div>
            <div class="visePresident2">
                <img src="../images/b1.jpg" alt="">
                <h3>Vise President</h3>
                <span>Shri Sanjay Anant</span>
            </div>
            <div class="visePresident3">
                <img src="../images/b1.jpg" alt="">
                <h3>Vise President (Finance)</h3>
                <span>Shri Anupam Sahay</span>
            </div>
        </div>

        <div class="swiper mySwiper">
          <div class="swiper-wrapper">
            <div class="swiper-slide">
              <img src="../images/b6.jpg" alt="">
              <br>
              <h3>Secretary</h3>
              <span>Shri Satish Singh</span>
            </div>
            <div class="swiper-slide">
              <img src="images/prabhat.jpg" alt="">
              <br>
              <h3>Joint Secretary</h3>
              <span>Shri Prabhat Srivastava</span>
            </div>
            <div class="swiper-slide">
              <img src="images/ashutosh.jpg" alt="">
              <br>
              <h3>Joint Secretary</h3>
              <span>Shri Ashutosh Srivastava</span>
            </div>
            <div class="swiper-slide">
              <img src="images/sunanda.jpg" alt="">
              <br>
              <h3 class="MediaInCharge">Media In charge </h3>
              <span>Ms. Sunanda Saxena </span>
            </div>
            <div class="swiper-slide">
              <img src="../images/b6.jpg" alt="">
              <br>
              <h3>Auditor</h3>
              <span>Ms. Jaspal Kaur</span>
            </div>

          </div>
          <div class="swiper-pagination"></div>
        </div>
        <br>
    </section>

    {{>footer}}
    <script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>
    <script>
      var swiper = new Swiper(".mySwiper", {
        slidesPerView: 3,
        spaceBetween: 30,
        freeMode: true,
        pagination: {
          el: ".swiper-pagination",
          clickable: true,
        },
      });
    </script>
    <script src="js/navbar.js"></script>
</body>
</html>